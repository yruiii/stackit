module compare (
    input z,
    input v,
    input n,
    input alufn_signal[6],
    output cmp
  ) {
  sig nxorv;
  always {
    // default values to silence errors 
    nxorv = n^v;
    case(alufn_signal[2:1]){
      b01: cmp = z;
      b10: cmp = nxorv;
      b11: cmp = z|nxorv;
      default: cmp = 0;
    }
  }
}
